hashsum-help-sha512 = lavora con SHA512
hashsum-help-sha3 = lavora con SHA3
hashsum-help-sha3-224 = lavora con SHA3-224
hashsum-help-sha3-256 = lavora con SHA3-256
hashsum-help-sha3-384 = lavora con SHA3-384
hashsum-help-sha3-512 = lavora con SHA3-512
hashsum-help-shake128 = lavora con SHAKE128 usando BITS per dimensione output
hashsum-help-shake256 = lavora con SHAKE256 usando BITS per dimensione output
